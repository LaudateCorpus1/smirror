{
  "Routes": [
    {
      "Prefix": "/data/p1",
      "Suffix": ".csv",
      "DestURL": "s3://${s3Bucket}/data",
      "OnCompletion": {
        "OnSuccess": [
          {
            "Action": "delete"
          }
        ],
        "OnError": [
          {
            "Action": "move",
            "URL": "gs:///${gsBucket}/e2e-mirror/errors/"
          }
        ]
      },
      "FolderDepth": 1
    },
    {
      "Prefix": "/data/p2",
      "Suffix": ".csv",
      "DestURL": "s3://${s3Bucket}/data",
      "Split": {
        "MaxLines": 10,
        "Template": "%s_%05d"
      },
      "OnCompletion": {
        "OnSuccess": [
          {
            "Action": "delete"
          }
        ],
        "OnError": [
          {
            "Action": "move",
            "URL": "gs:///${gsBucket}/e2e-mirror/errors/"
          }
        ]
      },
      "FolderDepth": 1
    }
  ],
  "Secrets": [
    {
      "TargetScheme": "s3",
      "URL": "gs://${gsBucket}/e2e-mirror/secret/s3-mirror.json.enc",
      "Key": "projects/my_project/locations/us-central1/keyRings/my_ring/cryptoKeys/my_key"
    }
  ]
}