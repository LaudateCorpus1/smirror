{
  "Routes": [
    {
      "Prefix": "/data/p1",
      "Suffix": ".csv",
      "DestURL": "gs://${s3Bucket}/data",
      "OnCompletion": {
        "OnSuccess": [
          {
            "Action": "delete"
          }
        ],
        "OnError": [
          {
            "Action": "move",
            "URL": "s3:///${gsBucket}/e2e-mirror/errors/"
          }
        ]
      },
      "FolderDepth": 1
    }
  ],
  "Secrets": [
    {
      "Provider": "aws",
      "TargetScheme": "s3",
      "Parameter": "smirror.gs",
      "Key": "alias/smirror"
    }
  ]
}