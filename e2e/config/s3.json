{
  "Routes": [
    {
      "Prefix": "/data/p5",
      "Suffix": ".csv",
      "Dest": {
        "URL": "gs://${s3Bucket}/data",
        "Credentials": {
          "Parameter": "smirror.gs",
          "Key": "alias/smirror"
        }
      },
      "OnSuccess": [
        {
          "Action": "delete"
        }
      ],
      "OnFailure": [
        {
          "Action": "move",
          "URL": "s3:///${gsBucket}/e2e-mirror/errors/"
        }
      ],
      "FolderDepth": 1
    }
  ]
}