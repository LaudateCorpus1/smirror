[
  {
    "Source": {
      "Prefix": "/data/p1",
      "Suffix": ".csv"
    },
    "Dest": {
      "URL": "s3://${s3DestBucket}/data",
      "Credentials": {
        "URL": "gs://${gsConfigBucket}/Secrets/s3-mirror.json.enc",
        "Key": "projects/${gcpProject}/locations/us-central1/keyRings/gs_mirror_ring/cryptoKeys/gs_mirror_key"
      }
    },
    "OnSuccess": [
      {
        "Action": "move",
        "URL": "gs://${gsOpsBucket}/StorageMirror/processed/"
      }
    ],

    "OnFailure": [
      {
        "Action": "move",
        "URL": "gs://${gsOpsBucket}/StorageMirror/errors/"
      }
    ],
    "PreserveDepth": 1
  }
]