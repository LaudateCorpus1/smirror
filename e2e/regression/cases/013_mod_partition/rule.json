[
  {
    "Source": {
      "Prefix": "/data/p13",
      "Suffix": ".csv"
    },
    "Dest": {

      "Topic": "${gsPrefix}_storage_mirror_p$partition"
    },
    "Split": {
      "MaxSize": 1048576,
      "Partition": {
          "FiledIndex": 0,
          "Mod": 2
      },
      "Template": "$name_$chunk_$partition"
    },
    "OnSuccess": [
      {
        "Action": "delete"
      }
    ],
    "OnFailure": [
      {
        "Action": "move",
        "URL": "gs:///${gsOpsBucket}/StorageMirror/errors/"
      }
    ],
    "PreserveDepth": 1
  }
]