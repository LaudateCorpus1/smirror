[
  {
    "Source": {
      "Prefix": "/data/p6",
      "Suffix": ".csv"
    },
    "Dest": {

      "Topic": "${gsPrefix}_storage_mirror"
    },
    "Split": {
      "MaxLines": 10
    },
    "OnSuccess": [
      {
        "Action": "delete"
      }
    ],
    "OnFailure": [
      {
        "Action": "move",
        "URL": "gs:///${gsOpsBucket}/StorageMirror/errors/"
      }
    ],
    "PreserveDepth": 1
  }
]