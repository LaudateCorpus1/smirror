[
  {
    "Source": {
      "Prefix": "/data/p7",
      "Suffix": ".csv",
      "CustomKey": {
        "Parameter": "storagemirror.customer001CustomKey",
        "Key": "alias/storagemirror"
      }
    },
    "Dest": {
      "URL": "gs://${gsDestBucket}/data",
      "Credentials": {
        "Parameter": "storagemirror.gcp",
        "Key": "alias/storagemirror"
      }
    },
    "OnSuccess": [
      {
        "Action": "delete"
      }
    ],
    "OnFailure": [
      {
        "Action": "move",
        "URL": "s3:///${s3OpsBucket}/StorageMirror/Errors/"
      }
    ],
    "PreserveDepth": 1
  }

]