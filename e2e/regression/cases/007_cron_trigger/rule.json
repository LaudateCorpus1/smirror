[
  {
    "Source": {
      "Prefix": "/${s3ExternalBucket}/",
      "Suffix": ".csv"
    },
    "Dest": {
      "URL": "gs://${gsDestBucket}/",
      "Credentials": {
        "Parameter": "storagemirror.gcp",
        "Key": "alias/storagemirror"
      }
    },
    "OnSuccess": [
      {
        "Action": "delete"
      }
    ],
    "OnFailure": [
      {
        "Action": "move",
        "URL": "s3:///${s3OpsBucket}/StorageMirror/Errors/"
      }
    ],
    "PreserveDepth": -1
  }
]