[
  {
    "Prefix": "/data/p7",
    "Suffix": ".csv",
    "Source": {

      "CustomKey": {
        "Parameter": "smirror.partnerXKey",
        "Key": "alias/smirror"
      }
    },
    "Dest": {
      "URL": "gs://${gsDestBucket}/data",
      "Credentials": {
        "Parameter": "smirror.gs",
        "Key": "alias/smirror"
      }
    },
    "OnSuccess": [
      {
        "Action": "delete"
      }
    ],
    "OnFailure": [
      {
        "Action": "move",
        "URL": "s3:///${s3OpsBucket}/e2e-mirror/errors/"
      }
    ],
    "FolderDepth": 1
  }

]