{
  "Mirrors": {
    "Rules": [
      {
        "Source": {
          "Prefix": "/data/p6",
          "Suffix": ".csv"
        },
        "Dest": {
          "Topic": "myTopic"
        },
        "Split": {
          "MaxLines": 1000
        },
        "OnSuccess": [
          {
            "Action": "delete"
          }
        ],
        "OnFailure": [
          {
            "Action": "move",
            "URL": "gs:///${gsTriggerBucket}/StorageMirror/errors/"
          }
        ],
        "PreserveDepth": 1
      }
    ]
  }
}