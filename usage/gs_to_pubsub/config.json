{
  "Routes": [
    {
      "Prefix": "/data/p6",
      "Suffix": ".csv",
      "Dest": {
        "Topic": "myTopic"
      },
      "Split": {
        "MaxLines": 1000
      },
      "OnSuccess": [
        {
          "Action": "delete"
        }
      ],
      "OnFailure": [
        {
          "Action": "move",
          "URL": "gs:///${gsBucket}/e2e-mirror/errors/"
        }
      ],
      "FolderDepth": 1
    }
  ]
}