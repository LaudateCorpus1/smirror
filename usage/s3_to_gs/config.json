{
  "Routes": [
    {
      "Prefix": "/data/",
      "Suffix": ".csv.gz",
      "Dest": {
        "URL": "gs://destBucket/data",
        "Credentials": {
          "Parameter": "smirror.gs",
          "Key": "smirror"
        }
      },
      "OnCompletion": {
        "OnSuccess": [
          {
            "Action": "delete"
          }
        ],
        "OnError": [
          {
            "Action": "move",
            "URL": "s3://sourceBucket/data/errors/"
          }
        ]
      },
      "Codec": "gzip",
      "FolderDepth": 1
    },
    {
      "Prefix": "/large/data/",
      "Suffix": ".csv.gz",
      "Dest": {
        "URL": "gs://destBucket/data/chunks/",
        "Credentials": {
          "Parameter": "smirror.gs",
          "Key": "smirror"
        }
      },
      "Split": {
        "MaxLines": 10000,
        "Template": "%s_%05d"
      },
      "OnCompletion": {
        "OnSuccess": [
          {
            "Action": "delete"
          }
        ],
        "OnError": [
          {
            "Action": "move",
            "URL": "s3://sourceBucket/data/errors/"
          }
        ]
      },
      "Codec": "gzip",
      "FolderDepth": 1
    }
  ]
}